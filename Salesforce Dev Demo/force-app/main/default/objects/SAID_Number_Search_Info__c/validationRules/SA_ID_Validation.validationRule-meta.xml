<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SA_ID_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(SA_ID_Number__c,&quot;/(([0-9]{2})(0|1)([0-9])([0-3])([0-9]))([ ]?)(([ 0-9]{4})([ ]?)([ 0-1][8]([ ]?)[ 0-9]))/&quot;))</errorConditionFormula>
    <errorDisplayField>SA_ID_Number__c</errorDisplayField>
    <errorMessage>Id Number does not seems authentic. Please add valid Id number.</errorMessage>
</ValidationRule>
